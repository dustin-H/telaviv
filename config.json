{
	"routes": [{
		"path": "/lol/:id",
		"html5": [{
			"component": "Header",
			"data": {
				"type": "REST",
				"url": "/api/header"
			}
		}, {
			"component": "Article",
			"data": {
				"type": "REST",
				"url": "/api/article/:id"
			},
			"options": {
				"id": ":id"
			}
		}, {
			"component": "Footer"
		}],
		"amphtml": [{
			"component": "Header",
			"data": {
				"type": "REST",
				"url": "/api/header"
			}
		}, {
			"component": "Article",
			"data": {
				"type": "REST",
				"url": "/api/article/:id"
			}
		}]
	}, {
		"path": "/static",
		"html5": [{
			"component": "Footer"
		}, {
			"component": "Footer"
		}, {
			"component": "Footer"
		}]
	}],
	"theme": "default",
	"apiAddress": "http://localhost:8000/"
	,
	"errors": [{
		"from": 401,
		"to": 401,
		"path": "/lol/ThisIsTheLoginError"
	}]
}
