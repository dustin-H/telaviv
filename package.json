{
  "name": "telaviv",
  "version": "0.0.2",
  "description": "Node.JS React Frontend Server",
  "main": "build/app.js",
  "scripts": {
    "start": "NODE_ENV=production node app.js",
    "build": "NODE_ENV=production babel src -d build",
    "build-and-start": "NODE_ENV=development npm run build && npm start",
    "prod-build-client": "NODE_ENV=production webpack -p --progress --colors",
    "prod-build-server": "NODE_ENV=production babel src -d build",
    "production": "npm run prod-build-server && npm run prod-build-client",
    "test": "istanbul cover _mocha -x \"src/router/pathMatcher.js\" -- test --opts testSetup/mocha.opts",
    "dev": "NODE_ENV=development webpack --progress --colors --watch"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "add-module-exports",
      "transform-react-display-name",
      "transform-decorators-legacy"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dustin-H/bauhausjs.git"
  },
  "author": "Dustin Hoffner",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dustin-H/bauhausjs/issues"
  },
  "homepage": "https://github.com/dustin-H/bauhausjs#readme",
  "dependencies": {
    "async": "^2.0.0-rc.2",
    "express": "^4.13.4",
    "history": "^2.0.1",
    "http-status": "^0.2.1",
    "lodash": "^4.0.0",
    "parse-cache-control": "^1.0.1",
    "react": "latest",
    "react-dom": "latest",
    "react-look": "/Users/hex0r/GitHub/react-look/packages/react-look",
    "react-look-scope": "0.0.1",
    "react-look-server-rendering": "0.0.1",
    "react-redux": "latest",
    "redux": "^3.3.1",
    "redux-thunk": "^1.0.3",
    "superagent": "^1.7.2"
  },
  "devDependencies": {
    "babel-cli": "^6.6.0",
    "babel-core": "^6.7.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.1.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-display-name": "^6.4.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-react-hmre": "^1.0.1",
    "babel-preset-stage-0": "^6.3.13",
    "bauhaus-ui-module-utils": "0.0.1",
    "chai": "^3.5.0",
    "esformatter-jsx": "^4.1.3",
    "istanbul": "^1.0.0-alpha.2",
    "mocha": "^2.4.5",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "webpack": "^1.12.14"
  },
  "esformatter": {
    "plugins": [
      "esformatter-jsx"
    ],
    "jsx": {
      "formatJSX": true,
      "attrsOnSameLineAsTag": true,
      "maxAttrsOnTag": 1,
      "firstAttributeOnSameLine": false,
      "spaceInJSXExpressionContainers": " ",
      "alignWithFirstAttribute": true,
      "htmlOptions": {
        "brace_style": "collapse",
        "indent_char": " ",
        "indent_size": 2,
        "max_preserve_newlines": 2,
        "preserve_newlines": true
      }
    }
  }
}
